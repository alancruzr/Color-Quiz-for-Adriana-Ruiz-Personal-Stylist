&lt;p&gt;&lt;span style="font-size: 30px;"&gt;&lt;/span&gt;&lt;span style="font-family: Helvetica; font-size: 30px;"&gt;&lt;span style="font-size: 30px;"&gt;&lt;span style="color: rgb(23, 54, 93);"&gt;&lt;span style="color: rgb(0, 0, 0);"&gt;&lt;span style="font-size: 30px;"&gt;&lt;span style="font-size: 30px;"&gt;&lt;br&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span style="font-family: Helvetica; font-size: 30px;"&gt;&lt;span style="font-size: 30px;"&gt;&lt;span style="color: rgb(23, 54, 93);"&gt;&lt;span style="color: rgb(0, 0, 0);"&gt;&lt;span style="font-size: 30px;"&gt;&lt;span style="font-size: 30px;"&gt;&lt;br&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span style="font-family: Helvetica; font-size: 30px;"&gt;&lt;span style="font-size: 30px;"&gt;&lt;span style="color: rgb(23, 54, 93);"&gt;&lt;span style="color: rgb(0, 0, 0);"&gt;&lt;span style="font-size: 30px;"&gt;&lt;span style="font-size: 30px;"&gt;&lt;span style="color: rgb(255, 255, 255);"&gt;..........&lt;/span&gt;Find out if your Color Analysis is Up to Date!&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
